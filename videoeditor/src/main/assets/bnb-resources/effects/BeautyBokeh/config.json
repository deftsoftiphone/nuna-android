{
  "name": "BeautyBokeh",
  "frx": {
    "type": "3d_anim",
    "3d_anim": {
      "type": "vfx"
    }
  },
  "foreground": {
     "type": "MEDIA",
     "media": {
       "type": "VIDEO",
       "file": "bokeh_foreground.mp4",
       "opacity": 1.0,
       "videoSpeed": 1.0,
       "blend": "SCREEN",
       "mode": "RGBA"
     }
   },
    "final": {
    "type": "FILTER",

    "filter": {
      "fragmentCode": "finalColorFilter.fsh",
      "textures": [{
        "file": "sunny_blue.png",
        "sampler": "lookupTexture",
        "magFilter": "LINEAR",
        "minFilter": "LINEAR"
      }]
    }
  },
    "backgroundSeparation": {
    "type": "MEDIA",
    "media": {
      "type": "VIDEO",
      "file": "bokeh_background.mp4",
      "videoSpeed": 1.0,
      "blend": "SCREEN",
      "mode": "RGBA"
    }
  },
    "sounds": [{
    "trackName": "music.ogg"
  }],
  "recordDuration": 15
}